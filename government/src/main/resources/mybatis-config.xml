<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!-- 전통적인 데이터베이스 칼럼명 형태인 A_COLUMN을 
          CamelCase형태의 자바 프로퍼티명 형태인 aColumn으로 자동으로 매핑하도록 함:매핑 false-->
        <setting name="mapUnderscoreToCamelCase" value="false"/>
    </settings>
    
    <typeAliases>
        <typeAlias type="com.sist.security.asset.AssetVO" alias="AssetVO"/>
        <typeAlias type="com.sist.security.cmn.SearchVO" alias="SearchVO"/>
        
    </typeAliases>
</configuration>  